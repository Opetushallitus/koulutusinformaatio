from operator import itemgetter
from data import s, get, out, dotted_getter

url = "/lo/search/*?start=0&rows=25&lang=fi&searchType=LO&facetFilters=teachingLangCode_ffm:FI&facetFilters=educationType_ffm:et02.02"

results = get(url)
if "results" in results:
	retval = sorted([get(s("/lo/special/{x[id]}")) for x in results["results"]], key=dotted_getter("name"))
else:
	retval = []
