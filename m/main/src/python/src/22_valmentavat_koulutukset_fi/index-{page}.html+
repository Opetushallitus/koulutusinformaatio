{% extends "index-base-fi.html" %}
{% block title %}{{ title_valmentavat_koulutukset_fi }}{% endblock %}
{% block content %}
<h1>{{ title_valmentavat_koulutukset_fi }}</h1>

<h2>Koulutukset</h2>

<ul>
  {% for item in ctx %}
  <li>
    <a href="{{ item.id }}.html">{{ item.name }} ({{ item.provider.name}})</a>
  </li>
  {% endfor %}
</ul>

{% endblock %}
