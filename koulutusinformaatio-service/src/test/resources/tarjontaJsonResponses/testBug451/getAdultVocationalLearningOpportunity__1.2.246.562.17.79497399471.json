        "tekstis" : {
          "kieli_sv" : "<p>Examen ger tillträde till högskolestudier.</p>",
          "kieli_fi" : "<p>Tutkinto tuottaa kelpoisuuden korkeakouluopintoihin.</p>"
        }
      },
      "TAVOITTEET" : {
        "kieliUri" : null,
